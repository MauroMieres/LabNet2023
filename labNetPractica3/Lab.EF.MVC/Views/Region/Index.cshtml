@model List<Lab.EF.MVC.Models.RegionViewModel>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <thead>
        <tr>
            <td>Id</td>
            <td>Description</td>
            <td>Eliminar</td>
            <td>Modificar</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td> @item.Id</td>
                <td> @item.Descripcion</td>
                <td> @Html.ActionLink("Eliminar", "Eliminar", new { id = @item.Id })</td>
                <td> @Html.ActionLink("Modificar", "Modificar", new { id = @item.Id })</td>
            </tr>
        }
    </tbody>
</table>

